extends ../layout

block content
  h1= title
  h2 Lista artykółów
  table
    tr
      th id
      th tytuł
      th Data dodania
      th akcja
    each item, index in data
      tr
        td=index
        td=item.title
        td=item.dd
        td 
          a(href=`/admin/news/delete/${item._id}`) Usuń